{
    "name": "smart-deps",
    "displayName": "SmartDeps",
    "description": "Keep your package.json and node_modules in sync",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "keywords": [
        "npm",
        "package.json",
        "dependencies",
        "dependency checker",
        "missing dependencies",
        "version mismatch",
        "nodejs",
        "node_modules",
        "monorepo",
        "workspace"
    ],
    "engines": {
        "vscode": "^1.109.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "prettier": "npx prettier . --write",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/semver": "^7.7.1",
        "@types/vscode": "^1.109.0",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.39.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0"
    },
    "dependencies": {
        "prettier": "^3.8.1",
        "semver": "^7.7.4"
    }
}
